             i2 + 0 = -100    and
            90 + s1 = 120     and
             -1 + 6 = 5       and
             1 + s2 = 0       and
           150 + i2 = 50      and
         0 + minint = minint  and
          -100 + s2 = -101    and
    maxint + minint = -1
