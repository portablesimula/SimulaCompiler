         0 * minint = 0       and
            i1 * i2 = -100    and
            s1 * i1 = -30     and
          -100 * s2 = -100    and
            i2 * s1 = 30      and
        maxint * s2 = maxint  and
            i1 * i1 = 100     and
            s1 * s2 = -3
