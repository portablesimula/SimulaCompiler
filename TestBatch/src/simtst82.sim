            s2 - i1 = -1      and
             0 - s2 = 1       and
            25 - s1 = 55      and
          -100 - i2 = -101    and
           -20 - s1 = 10      and
         minint - 0 = minint  and
           -i2 - s2 = 0       and
     maxint-maxint  = 0
