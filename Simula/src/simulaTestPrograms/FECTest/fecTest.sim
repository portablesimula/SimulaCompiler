%+D  text procedure C_code(k); character k;
%+D  begin 
%+D        if k = C_unknwn then C_code:-"default" else
%+D        if k = C_local  then C_code:-"local" else
%+D        if k = C_value  then C_code:-"value" else
%+D        if k = C_name   then C_code:-"name" else
