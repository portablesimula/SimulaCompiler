begin
	ref(OutByteFile) obf
	text tt="!010!";
	character cc;
	integer i,kk;
	text t;
	t:-copy(tt & "String");
	for i:=1 step 1 until 8 do begin
		cc:=t.getchar;
		kk:=rank(cc);
		outtext("Pos " & edit(i) & " " & edit(cc) & " = ");
		outint(kk,3);
		outimage;
	end;
	obf:-new OutByteFile("TempFile");
	obf.open;
	obf.outtext(t);
	obf.close;
end;